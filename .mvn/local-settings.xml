<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <servers>
        <server>
            <username>${env.JFROG_USER}</username>
            <password>${env.JFROG_MAVEN_PASS}</password>
            <id>central</id>
        </server>
        <server>
            <username>${env.JFROG_USER}</username>
            <password>${env.JFROG_MAVEN_PASS}</password>
            <id>snapshots</id>
        </server>
        <server>
            <username>${env.JFROG_USER}</username>
            <password>${env.JFROG_MAVEN_PASS}</password>
            <id>mirror-of-central</id>
        </server>
        <server>
            <username>${env.JFROG_USER}</username>
            <password>${env.JFROG_MAVEN_PASS}</password>
            <id>mirror-of-snapshots</id>
        </server>
    </servers>
    <profiles>
        <profile>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>libs-release</name>
                    <url>http://${env.JFROG_URL}/artifactory/libs-release</url>
                </repository>
                <repository>
                    <snapshots />
                    <id>snapshots</id>
                    <name>libs-snapshot</name>
                    <url>http://${env.JFROG_URL}/artifactory/libs-snapshot</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>libs-release</name>
                    <url>http://${env.JFROG_URL}/artifactory/libs-release</url>
                </pluginRepository>
                <pluginRepository>
                    <snapshots />
                    <id>snapshots</id>
                    <name>libs-snapshot</name>
                    <url>http://${env.JFROG_URL}/artifactory/libs-snapshot</url>
                </pluginRepository>
            </pluginRepositories>
            <id>artifactory</id>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
    <mirrors>
        <mirror>
            <id>mirror-of-central</id>
            <mirrorOf>central</mirrorOf>
            <name></name>
            <url>http://${env.JFROG_URL}/artifactory/libs-release</url>
            <blocked>false</blocked>
        </mirror>
        <mirror>
            <id>mirror-of-snapshots</id>
            <mirrorOf>snapshots</mirrorOf>
            <name></name>
            <url>http://${env.JFROG_URL}/artifactory/libs-snapshot</url>
            <blocked>false</blocked>
        </mirror>
    </mirrors>
</settings>